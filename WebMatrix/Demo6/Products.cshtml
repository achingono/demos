@{
    var pageLocation = "~/Shared/Products/";
    var pageName = "_List";
    var count = UrlData.Count();
    
    if (count >= 1){
        pageName = "_Details";
    }
    
    Page.Theme = "Default";
}

@section menu{
   @RenderPage("~/Shared/_Menu.cshtml") 
}

@RenderPage(string.Format("{0}{1}.cshtml", pageLocation, pageName))