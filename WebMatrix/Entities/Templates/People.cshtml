@{
    var people = DataContext.Current.People;
}
<table>
    <caption>All People</caption>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Group</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var person in people) {
            <tr>
                <td>@person.FirstName</td>
                <td>@person.LastName</td>
                <td>@person.Group.Name</td>
            </tr>
        }
    </tbody>
</table>